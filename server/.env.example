# Application
APP_ENV=development
APP_DEBUG=true
APP_SECRET=your-app-secret-key

# Database
DB_CONNECTION=mysql
DB_HOST=host.docker.internal
DB_PORT=3306
DB_DATABASE=openapi
DB_USERNAME=your_username
DB_PASSWORD=your_password

# Redis
REDIS_HOST=host.docker.internal
REDIS_PORT=6379
REDIS_PASSWORD=null

# JWT
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRATION=3600

# CORS
CORS_ALLOWED_ORIGINS=*
CORS_ALLOWED_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_ALLOWED_HEADERS=Content-Type,Authorization,X-Requested-With,X-App-ID

# Logging
LOG_CHANNEL=stack
LOG_LEVEL=debug
